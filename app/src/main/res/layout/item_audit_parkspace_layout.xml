<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/w0">

    <TextView
        android:id="@+id/my_parkspace_description"
        style="@style/TwentyTextB1Style"
        android:layout_width="0dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="14dp"
        android:layout_marginTop="18dp"
        android:ellipsize="end"
        android:gravity="start"
        android:maxLines="1"
        app:layout_constraintEnd_toStartOf="@+id/audit_parkspace_status"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/my_parkspace_park_location"
        style="@style/TwentyTextG6Style"
        android:layout_width="0dp"
        android:layout_marginBottom="14dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="10dp"
        android:ellipsize="end"
        android:gravity="start"
        android:maxLines="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/audit_parkspace_install_time"
        app:layout_constraintStart_toStartOf="@id/my_parkspace_description"
        app:layout_constraintTop_toBottomOf="@+id/my_parkspace_description" />

    <TextView
        android:id="@+id/audit_parkspace_install_time"
        style="@style/TenTextB1Style"
        android:layout_marginEnd="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/my_parkspace_park_location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/my_parkspace_park_location" />

    <TextView
        android:id="@+id/audit_parkspace_status"
        style="@style/TenTextB1Style"
        app:layout_constraintBottom_toBottomOf="@+id/my_parkspace_description"
        app:layout_constraintEnd_toEndOf="@+id/audit_parkspace_install_time"
        app:layout_constraintTop_toTopOf="@+id/my_parkspace_description" />

</android.support.constraint.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <include
        android:id="@+id/base_tb"
        layout="@layout/base_toolbar"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/top_divider"
        style="@style/dividerStyle"
        app:layout_constraintTop_toBottomOf="@+id/base_tb" />

    <ImageView
        android:layout_width="20dp"
        app:layout_constraintTop_toBottomOf="@+id/top_divider"
        android:layout_marginTop="15dp"
        android:id="@+id/park_space_info_iv"
        android:src="@drawable/ic_into"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/time_setting_iv"
        android:contentDescription="@string/picture_description"
        android:layout_height="20dp" />

    <ImageView
        android:layout_width="20dp"
        app:layout_constraintTop_toTopOf="@+id/park_space_info_iv"
        android:id="@+id/time_setting_iv"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:src="@drawable/ic_into"
        android:contentDescription="@string/picture_description"
        android:layout_height="20dp" />

    <ImageView
        android:layout_width="20dp"
        app:layout_constraintTop_toTopOf="@+id/park_space_info_iv"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/time_setting_iv"
        android:src="@drawable/ic_order_money"
        android:contentDescription="@string/picture_description"
        android:layout_height="20dp" />


</android.support.constraint.ConstraintLayout>
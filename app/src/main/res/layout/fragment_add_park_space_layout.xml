<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/solid_w0_corner_5dp">

    <TextView
        android:id="@+id/add_park_space_tv"
        style="@style/ThirtyTextB1Style"
        android:paddingBottom="16dp"
        android:paddingStart="6dp"
        android:paddingTop="16dp"
        android:text="添加个人车位"
        android:textColor="@color/y3"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tuzhao.publicwidget.customView.PlusView
        android:id="@+id/add_park_space_pv"
        android:layout_width="28dp"
        android:layout_height="44dp"
        android:paddingBottom="16dp"
        android:paddingStart="16dp"
        android:paddingTop="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/add_park_space_tv"
        app:layout_constraintEnd_toStartOf="@id/add_park_space_tv"
        app:layout_constraintTop_toTopOf="@+id/add_park_space_tv"
        app:plus_color="@color/y3"
        app:plus_width="0.5dp" />

</android.support.constraint.ConstraintLayout>
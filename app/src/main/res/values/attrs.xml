<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--下拉刷新refresh-->
    <declare-styleable name="SwipeToLoadLayout">
        <attr name="refresh_enabled" format="boolean" />
        <attr name="load_more_enabled" format="boolean" />
        <attr name="swipe_style" format="enum">
            <enum name="classic" value="0" />
            <enum name="above" value="1" />
            <enum name="blew" value="2" />
            <enum name="scale" value="3" />
        </attr>
        <attr name="drag_ratio" format="float" />
        <attr name="refresh_trigger_offset" format="dimension" />
        <attr name="load_more_trigger_offset" format="dimension" />
        <attr name="refresh_final_drag_offset" format="dimension" />
        <attr name="load_more_final_drag_offset" format="dimension" />
        <attr name="swiping_to_refresh_to_default_scrolling_duration" format="integer" />
        <attr name="release_to_refreshing_scrolling_duration" format="integer" />
        <attr name="refresh_complete_delay_duration" format="integer" />
        <attr name="refresh_complete_to_default_scrolling_duration" format="integer" />
        <attr name="default_to_refreshing_scrolling_duration" format="integer" />
        <attr name="swiping_to_load_more_to_default_scrolling_duration" format="integer" />
        <attr name="release_to_loading_more_scrolling_duration" format="integer" />
        <attr name="load_more_complete_delay_duration" format="integer" />
        <attr name="load_more_complete_to_default_scrolling_duration" format="integer" />
        <attr name="default_to_loading_more_scrolling_duration" format="integer" />
    </declare-styleable>

    <!--订单进度条-->
    <declare-styleable name="TimeLineView">
        <attr name="startedLineColor" format="color" />
        <attr name="preLineColor" format="color" />
        <attr name="startedCircleColor" format="color" />
        <attr name="underwayCircleColor" format="color" />
        <attr name="preCircleColor" format="color" />
        <attr name="startedStringColor" format="color" />
        <attr name="underwayStringColor" format="color" />
        <attr name="preStringColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="radius" format="dimension" />
        <attr name="lineWidth" format="dimension" />
    </declare-styleable>

    <!--圆形图片-->
    <declare-styleable name="CircleImageView">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <declare-styleable name="Voltage">
        <attr name="border_width" format="dimension" />
        <attr name="voltage" format="integer" />
        <attr name="border_radius" format="dimension" />
        <attr name="border_color" format="color" />
        <attr name="low_voltage_color" format="color" />
        <attr name="middle_voltage_color" format="color" />
        <attr name="heigh_voltage_color" format="color" />
    </declare-styleable>

    <declare-styleable name="NavigationView">
        <attr name="background_color" format="color" />
        <attr name="inner_color" format="color" />
        <attr name="inner_radius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleView">
        <attr name="color" format="color" />
    </declare-styleable>

    <declare-styleable name="VoltageView">
        <attr name="vv_border_width" format="dimension" />
        <attr name="vv_voltage" format="integer" />
        <attr name="vv_border_radius" format="dimension" />
        <attr name="vv_border_color" format="color" />
        <attr name="vv_low_voltage_color" format="color" />
        <attr name="vv_middle_voltage_color" format="color" />
        <attr name="vv_heigh_voltage_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CircularArcView">
        <attr name="stroke_width" format="dimension" />
        <attr name="CircularArcView_arc_color" format="color" />
        <attr name="CircularArcView_circle_color" format="color" />
        <attr name="CircularArcView_circle_radius" format="dimension" />
        <attr name="sweep_angle" format="integer" />
    </declare-styleable>

    <declare-styleable name="OvalView">
        <attr name="oval_color" format="color" />
    </declare-styleable>

    <declare-styleable name="CreditView">
        <attr name="flag_color" format="color" />
        <attr name="very_poor_color" format="color" />
        <attr name="poor_color" format="color" />
        <attr name="fine_color" format="color" />
        <attr name="good_color" format="color" />
        <attr name="very_good_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ArrowView">
        <attr name="arrow_color" format="color" />
        <attr name="arrow_width" format="dimension" />
        <attr name="arrow_gravity">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="left" value="2" />
            <enum name="right" value="3" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CrossView">
        <attr name="cross_color" format="color" />
        <attr name="cross_width" format="dimension" />
        <attr name="draw_circle" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PlusView" >
        <attr name="plus_color" format="color" />
        <attr name="plus_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="PasswordLinearLayout">
        <attr name="child_count" format="integer" />
        <attr name="child_margin" format="dimension" />
        <attr name="child_width" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CornerImageView">
        <attr name="corner_radius" format="dimension" />
        <attr name="corner_color" format="color" />
        <attr name="discount_text_color" format="color" />
        <attr name="discount" format="string" />
        <attr name="corner_text_color" format="color" />
        <attr name="corner_text_size" format="dimension" />
        <attr name="corner_text_horizontal_margin" format="dimension" />
        <attr name="corner_text_bottom_margin" format="dimension" />
    </declare-styleable>

</resources>